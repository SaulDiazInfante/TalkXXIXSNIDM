@Article{Blayneh2009,
  Title                    = {{Optimal control of vector-borne diseases:
Treatment and prevention}},
  Author                   = {Blayneh, Kbenesh and Cao, Yanzhao and Kwon,
Hee-Dae},
  Journal                  = {Discrete and Continuous Dynamical Systems - Series

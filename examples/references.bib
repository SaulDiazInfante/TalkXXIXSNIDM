@Article{Piunovskiy2008,
  Title                    = {{An explicit optimal isolation policy for a
deterministic epidemic model}},
  Author                   = {Clancy, Damian and Piunovskiy, Alexei B.},
  Journal                  = {Applied Mathematics and Computation},
  Year                     = {2005},

  Month                    = {apr},
  Number                   = {3},
  Pages                    = {1109--1121},
  Volume                   = {163},

  Abstract                 = {Optimal policies involving the isolation of
infectives are derived for a deterministic epidemic model with non-standard
infection rate function. Denoting by y the number of infective individuals and x
the number of susceptible individuals in the population, we replace the
classical Kermack-McKendrick infection rate function $\beta$xy (for some
constant $\beta$) with $\beta$xy/(x + y). This modified model has been studied
by various previous authors, but not in the context of control policies. We show
that the optimal isolation policy is to intervene with maximal effort when y ≤
Ax, and not to intervene otherwise, for some constant A whose value can be given
explicitly in terms of the parameters of the model. {\textcopyright} 2004
Elsevier Inc. All rights reserved.},
  Doi                      = {10.1016/j.amc.2004.06.028},
